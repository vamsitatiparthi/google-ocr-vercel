# Copy to .env.local for Next.js
# Provide Google Cloud Vision credentials in one of the two ways below.

# Option 1: Plain JSON string of the service account key (recommended on Vercel)
# GOOGLE_CLOUD_CREDENTIALS={
#   "type": "service_account",
#   "project_id": "your-project-id",
#   "private_key_id": "...",
#   "private_key": "-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n",
#   "client_email": "your-sa@your-project-id.iam.gserviceaccount.com",
#   "client_id": "...",
#   "auth_uri": "https://accounts.google.com/o/oauth2/auth",
#   "token_uri": "https://oauth2.googleapis.com/token",
#   "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
#   "client_x509_cert_url": "..."
# }

# Option 2: Base64-encoded JSON string of the service account key
# GOOGLE_CLOUD_CREDENTIALS_BASE64=
